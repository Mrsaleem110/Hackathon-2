{
  "version": 2,
  "builds": [
    {
      "src": "app_serverless_crash_fix.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.11",
        "installCommand": "pip install -r requirements-vercel.txt"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/auth/(.*)",
      "dest": "app_serverless_crash_fix.py"
    },
    {
      "src": "/auth/(.*)",
      "dest": "app_serverless_crash_fix.py"
    },
    {
      "src": "/api/(.*)",
      "dest": "app_serverless_crash_fix.py"
    },
    {
      "src": "/(.*)",
      "dest": "app_serverless_crash_fix.py"
    }
  ],
  "functions": {
    "app_serverless_crash_fix.py": {
      "maxDuration": 10,
      "memory": 512,
      "includeFiles": "requirements-vercel.txt"
    }
  },
  "regions": ["dub1"]
}