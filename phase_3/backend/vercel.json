{
  "version": 2,
  "builds": [
    {
      "src": "serverless_stable.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.11",
        "installCommand": "pip install -r requirements-vercel.txt"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/auth/(.*)",
      "dest": "serverless_stable.py"
    },
    {
      "src": "/auth/(.*)",
      "dest": "serverless_stable.py"
    },
    {
      "src": "/api/(.*)",
      "dest": "serverless_stable.py"
    },
    {
      "src": "/(.*)",
      "dest": "serverless_stable.py"
    }
  ],
  "functions": {
    "serverless_stable.py": {
      "maxDuration": 3,
      "memory": 128,
      "includeFiles": "requirements-vercel.txt"
    }
  },
  "regions": ["iad1"]
}